<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ufl.srproject" > <!-- Target for any users using Jelybean or above, set min SDK to Gingerbread -->
    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" >
    </uses-sdk>

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.ufl.srproject.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.ufl.srproject.body_area_workout"
            android:label="@string/title_activity_body_area_workout" >
        </activity>
        <activity
            android:name="com.example.ufl.srproject.presetRandomizers"
            android:label="@string/presetRandomizer" >
        </activity>
    </application>

</manifest>
